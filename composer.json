{
    "name": "mk-ryan1988/eloquent-gaql",
    "description": "Allows you to write Google Ad's GAQL the Eloquent way.",
    "keywords": [
        "mk-ryan1988",
        "laravel",
        "eloquent-gaql"
    ],
    "homepage": "https://github.com/mk-ryan1988/eloquent-gaql",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Ryan",
            "email": "mkryan1988@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "MkRyan1988\\GaqlBuilder\\": "src",
            "MkRyan1988\\GaqlBuilder\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MkRyan1988\\GaqlBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MkRyan1988\\GaqlBuilder\\GaqlBuilderServiceProvider"
            ],
            "aliases": {
                "GaqlBuilder": "MkRyan1988\\GaqlBuilder\\GaqlBuilderFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
